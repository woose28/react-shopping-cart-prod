{
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 90,
  "arrowParens": "always",
  "trailingComma": "all",
  "endOfLine": "auto",
  "singleQuote": true,
  "importOrder": [
    "^(@|store)(.*|$)",
    "^(@|hooks)(.*|$)",
    "^(@|api)(.*|$)",
    "^(@|mocks)(.*|$)",
    "^(@|pages)(.*|$)",
    "^(@|components/common)(.*|$)",
    "^(@|components)(.*|$)",
    "^(@|constants)(.*|$)",
    "^(@|styles)(.*|$)"
  ],
  "importOrderSeparation": true
}
